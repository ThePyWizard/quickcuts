{
    "manifest_version": 3,
    "name": "QuickCuts",
    "version": "1.0",
    "permissions": ["activeTab", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  